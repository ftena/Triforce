<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	package="comm.tarlic.triforce" android:versionCode="9"
	android:versionName="2.0">

<uses-sdk android:minSdkVersion="9" 
          android:targetSdkVersion="17"
          android:maxSdkVersion="17" />

<uses-permission android:name="android.permission.READ_CALL_LOG"/>

<uses-permission android:name="android.permission.READ_CONTACTS"/>

<application android:label="@string/app_name"
    android:icon="@drawable/ic_launcher_triforce"
    android:allowBackup="true">"
	
	<!--
		Por cada Activity instanciada durante la ejecución de la aplicación,
		es necesario un <activity> ... </activity>
	-->

	<activity
	    android:name="comm.tarlic.triforce.TriforceBatteryInfoList"
	    android:label="@string/app_name"
	    android:theme="@android:style/Theme.Light">
		
		<intent-filter>
			<action android:name="android.intent.action.MAIN" />
			<category android:name="android.intent.category.LAUNCHER" />
		</intent-filter>
	
	</activity>
	
	<activity
        android:name="comm.tarlic.triforce.DisplayConfigActivity"
        android:label="@string/title_activity_display_config"
        android:theme="@android:style/Theme.Light">
    </activity>
	
</application>
        
</manifest> 


